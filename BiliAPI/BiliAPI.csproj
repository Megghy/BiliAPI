<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>Megghy</Authors>
    <Description>简单封装的B站常用API</Description>
    <PackageProjectUrl>https://github.com/Megghy/BiliAPI</PackageProjectUrl>
    <PackageTags>bilibili;bilibili-api;api;哔哩哔哩;b站;b站api</PackageTags>
    <AssemblyVersion>1.0.3</AssemblyVersion>
    <FileVersion>1.0.3</FileVersion>
    <RepositoryUrl>https://github.com/Megghy/BiliAPI</RepositoryUrl>
    <Version>$(VersionPrefix)</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\.editorconfig" Link=".editorconfig" />
  </ItemGroup>

</Project>
